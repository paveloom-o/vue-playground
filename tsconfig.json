{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "checkJs": false,
    "lib": ["DOM", "WebWorker", "ES2017"],
    "module": "ESNext",
    "target": "ES2017",
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
